{
  "name": "twinpines",
  "version": "1.0.0",
  "description": "Landing page for Twin Pines",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "devServer": "",
    "devStart": "npm-run-all --parallel start watch:sass",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/icon-font.css public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' public/css/style.concat.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysbrame/twinPinesBrewingCo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rhysbrame/twinPinesBrewingCo/issues"
  },
  "homepage": "https://github.com/rhysbrame/twinPinesBrewingCo#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "concat": "^1.0.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "express": "^4.17.1"
  }
}